{
  "presets": ["react", "stage-1", "es2015"],
  "env": {
    "development": {
      "plugins": [
        ["react-transform",
          {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": ["react"],
                "locals": ["module"]
              }
            ]
          }
        ]
      ]
    }
  }
}
